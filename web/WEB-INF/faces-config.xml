<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <locale-config>
            <default-locale>hr</default-locale>
            <supported-locale>en</supported-locale>
        </locale-config>
        <resource-bundle>
            <base-name>org.foi.nwtis.nikfluks.prijevod</base-name>
            <var>p</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/registracija.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>prijava</from-outcome>
            <to-view-id>/prijava.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/prijava.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pig</from-outcome>
            <to-view-id>/posluziteljIGrupa.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
